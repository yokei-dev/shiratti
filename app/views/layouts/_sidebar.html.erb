<div class="listings">
  <div class="listings_channels">
    <h2 class="listings_header">参加中のプロジェクト</h2>
    <ul class="channel_list">

      <% current_user.joinings.each do |p| %>
      <div class="panel-group channel">
        <div class="panel panel-default">
          <div class="panel-heading">
            <div class="panel-title">
              <a data-toggle="collapse" href="#collapse<%= p.id %>"><%= p.name %></a>
            </div>
          </div>
          <div id="collapse<%= p.id %>" class="panel-collapse collapse">
            <ul class="list-group">
              <li class="list-group-item">
                <%= link_to 'タスク', project_path(p), class: 'form-control btn btn-secondary', data: {"turbolinks"=>false} %>
              </li>
              <li class="list-group-item">
                <%= link_to 'メンバー', users_project_path(p), class: 'form-control btn btn-secondary', data: {"turbolinks"=>false}%>
              </li>
            </ul>
          </div>
        </div>
      </div>
      <% end %>

    </ul>
  </div>
</div>
