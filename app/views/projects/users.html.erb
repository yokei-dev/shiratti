<div class="container">
  <h2 class='mt-3'><%= @project.name%>のメンバー</h2>
  <% @users.each do |user| %>
    <div class='project'>
      <div class="row">
        <div  class="col-md-6">
          <p><%= user.name%></p>
        </div>
        <div class="offset-md-4 col-md-2">
          <%= link_to 'タスク状況', user, class: 'btn btn-primary' %>
        </div>
      </div>
    </div>
  <% end %>
</div>